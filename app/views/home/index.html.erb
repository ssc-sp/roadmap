<% if !user_signed_in? %>
  <div class="row">
    <div class="col-md-8">
      <%= render partial: 'home/welcome' %>
    </div>

    <div class="col-md-4">
      <%= render partial: 'shared/access_controls', locals: {orgs: @all_orgs, org_partial: @org_partial} %>
    </div>
  </div>
<% else %>
  <div class="row">
    <div class="col-md-12">
      <%= render partial: 'home/welcome' %>
    </div>
  </div>
<% end %>

